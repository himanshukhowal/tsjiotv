{
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.3.1"
    }
  },
  "routes": [
    {
      "src": "/assets/(.*)",
      "headers": {
        "cache-control": "s-maxage=604800"
      },
      "dest": "/assets/$1"
    },
    {
      "src": "/main",
      "dest": "api/index.php"
    },
    {
      "src": "/play\\.php(?<id>[^/]+)",
      "dest": "api/play.php$id"
    }
  ]
}
